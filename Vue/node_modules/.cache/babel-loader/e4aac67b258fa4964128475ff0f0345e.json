{"ast":null,"code":"import { mapMutations } from 'vuex';\nimport axios from 'axios';\nexport default {\n  name: 'UserLogin',\n  props: {\n    msg: String\n  },\n\n  data() {\n    return {\n      loginData: {\n        username: '',\n        password: ''\n      }\n    };\n  },\n\n  // this.axios({\n  //           method: 'post',\n  //           url: '/user/login',\n  //           data: _this.loginForm\n  //         })\n  methods: { ...mapMutations(['changeLogin']),\n\n    submit() {\n      this.$refs['loginForm'].validate(valid => {\n        if (valid) {\n          axios.post('http://localhost:8081/login', this.loginData).then(res => {\n            console.log(res.data);\n            this.userToken = 'Bearer ' + res.data.data.body.token; // 将用户token保存到vuex中\n\n            this.changeLogin({\n              Authorization: this.userToken\n            }); //传参内容为一个对象\n\n            alert('登陆成功');\n\n            if (this.loginData.password == '123456') {\n              alert('初次登录，请重置密码');\n              this.$router.push({\n                path: \"/password\"\n              });\n            } else this.$router.push({\n              path: \"/student\"\n            });\n          }).catch(error => {\n            alert('账号或密码错误');\n            console.log(error);\n          });\n        } else {\n          console.log('error submit');\n          return false;\n        }\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAsCA,SAASA,YAAT,QAA6B,MAA7B;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,eAAe;AACbC,MAAI,EAAE,WADO;AAEbC,OAAK,EAAE;AACLC,OAAG,EAAEC;AADA,GAFM;;AAKbC,MAAI,GAAG;AACL,WAAO;AACLC,eAAS,EAAE;AACTC,gBAAQ,EAAC,EADA;AAETC,gBAAQ,EAAC;AAFA;AADN,KAAP;AAMD,GAZY;;AAab;AACA;AACA;AACA;AACA;AACAC,SAAO,EAAE,EACN,GAAGV,YAAY,CAAC,CAAC,aAAD,CAAD,CADT;;AAEPW,UAAM,GAAG;AACP,WAAKC,KAAL,CAAW,WAAX,EAAwBC,QAAxB,CAAiCC,KAAI,IAAK;AACxC,YAAGA,KAAH,EAAU;AACRb,eAAK,CAACc,IAAN,CAAW,6BAAX,EAA0C,KAAKR,SAA/C,EACCS,IADD,CACMC,GAAE,IAAK;AACXC,mBAAO,CAACC,GAAR,CAAYF,GAAG,CAACX,IAAhB;AACA,iBAAKc,SAA