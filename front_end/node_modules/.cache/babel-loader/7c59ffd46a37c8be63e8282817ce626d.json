{"ast":null,"code":"import { ElMessage, ElMessageBox } from 'element-plus';\nexport default {\n  data() {\n    return {\n      rules: {\n        courseName: [{\n          required: true,\n          message: '请输入课程名称',\n          trigger: 'blur'\n        }],\n        courseInfo: [{\n          required: true,\n          message: '请输入课程介绍',\n          trigger: 'blur'\n        }]\n      },\n      editTableVisible: false,\n      dialogTableVisible: false,\n      selectTime: [[]],\n      roomProps: {\n        children: 'room',\n        label: 'name',\n        value: 'name'\n      },\n      classroom: [{\n        name: '第三教学楼',\n        aka: 'H3',\n        room: [{\n          name: '301'\n        }, {\n          name: '402'\n        }]\n      }, {\n        name: '光华楼西辅楼',\n        aka: 'HGX',\n        room: [{\n          name: '201'\n        }, {\n          name: '502'\n        }]\n      }],\n      courses: [{\n        courseId: 1,\n        courseName: '软件工程',\n        courseNum: 'SOFT220011',\n        classHours: '4',\n        credits: '4',\n        collegeId: '',\n        collegeName: '计算机与技术学院',\n        times: [[], [1, 2, 3], [3, 5], []],\n        courseInfo: '123',\n        status: 'normal'\n      }, {\n        courseId: 2,\n        courseName: '离散数学',\n        courseNum: 'SOFT220022',\n        classHours: '3',\n        credits: '5',\n        collegeId: '',\n        collegeName: '计算机与技术学院',\n        times: [[], [3, 4], [3, 8], []],\n        courseInfo: '123',\n        status: 'normal'\n      }],\n      editCourse: {\n        index: '',\n        courseName: '',\n        classroom: '',\n        times: ''\n      }\n    };\n  },\n\n  methods: {\n    // eslint-disable-next-line\n    tableRowClassName(row, rowIndex) {\n      //根据该行课程的状态动态显示该行表格颜色\n      if (row.row.status == 'changed') return 'changing-row';\n      if (row.row.status == 'deleted') return 'deleting-row';\n    },\n\n    handleDelete(index, data) {\n      ElMessageBox.confirm('是否申请删除课程\"' + data.courseName + '\"', '请确认', {\n        confirmButtonText: '确认',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        data.status = 'deleted';\n        ElMessage({\n          type: 'success',\n          message: '已发起申请'\n        });\n      }).catch(() => {\n        ElMessage({\n          type: 'info',\n          message: '申请取消'\n        });\n      });\n    },\n\n    handleEdit(index, data) {\n      //axios获取教室\n      this.editCourse.index = index;\n      this.editCourse.courseName = data.courseName;\n      this.editTableVisible = true;\n    },\n\n    submitEdit() {\n      this.$refs['editCourse'].validate(valid => {\n        if (valid) {\n          this.courses[this.editCourse.index].courseName = this.editCourse.courseName;\n          this.courses[this.editCourse.index].status = 'changed';\n          this.editTableVisible = false;\n        }\n      });\n    },\n\n    cancelEdit() {\n      this.editTableVisible = false;\n    },\n\n    test() {\n      this.courses[0].status = 'changed';\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAmDA,SAASA,SAAT,EAAoBC,YAApB,QAAwC,cAAxC;AACA,eAAe;AACXC,MAAI,GAAG;AACH,WAAO;AACHC,WAAK,EAAE;AACHC,kBAAU,EAAE,CAAC;AAACC,kBAAQ,EAAE,IAAX;AAAiBC,iBAAO,EAAE,SAA1B;AAAqCC,iBAAO,EAAE;AAA9C,SAAD,CADT;AAEHC,kBAAU,EAAE,CAAC;AAACH,kBAAQ,EAAE,IAAX;AAAiBC,iBAAO,EAAE,SAA1B;AAAqCC,iBAAO,EAAE;AAA9C,SAAD;AAFT,OADJ;AAKHE,sBAAgB,EAAC,KALd;AAMHC,wBAAkB,EAAC,KANhB;AAOHC,gBAAU,EAAC,CAAC,EAAD,CAPR;AAQHC,eAAS,EAAE;AACPC,gBAAQ,EAAE,MADH;AAEPC,aAAK,EAAE,MAFA;AAGPC,aAAK,EAAE;AAHA,OARR;AAaHC,eAAS,EAAE,CACP;AACIC,YAAI,EAAE,OADV;AAEIC,WAAG,EAAE,IAFT;AAGIC,YAAI,EAAE,CACF;AAACF,cAAI,EAAC;AAAN,SADE,EAEF;AAACA,cAAI,EAAC;AAAN,SAFE;AAHV,OADO,EASP;AACIA,YAAI,EAAE,QADV;AAEIC,WAAG,EAAE,KAFT;AAGIC,YAAI,EAAE,CACF;AAACF,cAAI,EAAC;AAAN,SADE,EAEF;AAACA,cAAI,EAAC;AAAN,SAFE;AAHV,OATO,CAbR;AA+BHG,aAAO,EAAE,CACL;AACIC,gBAAQ,EAAE,CADd;AAEIjB,kBAAU,EAAE,MAFhB;AAGIkB,iBAAS,EAAE,YAHf;AAIIC,kBAAU,EAAE,GAJhB;AAKIC,eAAO,EAAE,GALb;AAOIC,iBAAS,EAAE,EAPf;AAQIC,mBAAW,EAA