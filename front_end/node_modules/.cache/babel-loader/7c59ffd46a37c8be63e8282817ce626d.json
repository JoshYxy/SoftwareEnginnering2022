ourseName","required","message","trigger","courseInfo","editTableVisible","dialogTableVisible","selectTime","roomProps","children","label","value","classroom","name","aka","room","courses","courseId","courseNum","classHours","credits","collegeId","collegeName","times","status","editCourse","index","methods","tableRowClassName","row","rowIndex","handleDelete","confirm","confirmButtonText","cancelButtonText","type","then","catch","handleEdit","submitEdit","$refs","validate","valid","cancelEdit","test"],"sourceRoot":"","sources":["D:\\fdu\\software_engineering\\Lab\\5-Lab\\front_end\\src\\components\\course\\TeacherCourse.vue"],"sourcesContent":["<template>\r\n    <h2>我的课程</h2>\r\n    <el-button @click=\"test\">test </el-button>\r\n    <el-table class=\"table\" :data=\"courses\" :row-class-name=\"tableRowClassName\">\r\n        <el-table-column fixed prop=\"courseName\" label=\"课程名\" width=\"120\" />\r\n        <el-table-column fixed prop=\"courseNum\" label=\"课程编号\" width=\"120\" />\r\n        <el-table-column prop=\"collegeName\" label=\"开课院系\" width=\"180\" />\r\n        <el-table-column prop=\"classHours\" label=\"学时\" width=\"60\" />\r\n        <el-table-column prop=\"credits\" label=\"学分\" width=\"60\" />\r\n        <el-table-column prop=\"classroom\" label=\"上课地点\" width=\"140\" />\r\n        <el-table-column prop=\"times\" label=\"上课时间\" width=\"180\" />\r\n        <el-table-column prop=\"capacity\" label=\"选课容量\" width=\"150\" />\r\n        <el-table-column prop=\"courseInfo\" label=\"介绍\" width=\"150\" >\r\n            <template #default=\"scope\">\r\n                <el-button type=\"text\" @click=\"dialogTableVisible = true\">查看详情</el-button>\r\n                <el-dialog v-model=\"dialogTableVisible\" title=\"课程介绍\" :append-to-body=\"true\">\r\n                   {{scope.row.courseInfo}}\r\n                </el-dialog>\r\n            </template>\r\n        </el-table-column>\r\n        <el-table-column fixed=\"right\" label=\"操作\" width=\"180\">\r\n        <template #default=\"scope\">\r\n            <el-button size=\"small\" @click=\"handleEdit(scope.$index, scope.row)\">申请修改</el-button>\r\n            <el-button size=\"small\" type=\"danger\" @click=\"handleDelete(scope.$index, scope.row)\">申请删除</el-button>\r\n\r\n        </template>\r\n        </el-table-column>\r\n    </el-table>  \r\n    <el-dialog v-model=\"editTableVisible\" title=\"修改课程\" :append-to-body=\"true\">\r\n        <el-form \r\n            :model=\"editCourse\" \r\n            :rules=\"rules\" \r\n            hide-required-asterisk\r\n            ref=\"editCourse\"\r\n            >\r\n                <el-form-item prop=\"courseName\" label=\"课程名称\">\r\n                    <el-input type=\"text\" name=\"courseName\" v-model=\"editCourse.courseName\"/>\r\n                </el-form-item>\r\n                <el-form-item label=\"上课教室\" prop=\"classroom\">\r\n                    <el-cascader :props=\"roomProps\" :options=\"classroom\" v-model=\"selectRoom\" placeholder=\"可输入教室号搜索\" filterable clearable/>     \r\n                </el-form-item>\r\n        </el-form>\r\n        \r\n        <span class=\"dialog-footer\">\r\n            <el-button @click=\"cancelEdit\">取消</el-button>\r\n            <el-button type=\"primary\" @click=\"submitEdit\">确认</el-button>\r\n        </span>\r\n    </el-dialog>\r\n</template>\r\n\r\n<script>\r\nimport { ElMessage, ElMessageBox } from 'element-plus'\r\nexport default {\r\n    data() {\r\n        return {\r\n            rules: {\r\n                courseName: [{required: true, message: '请输入课程名称', trigger: 'blur'}],\r\n                courseInfo: [{required: true, message: '请输入课程介绍', trigger: 'blur'}],\r\n            },\r\n            editTableVisible:false,\r\n            dialogTableVisible:false,\r\n            selectTime:[[]],\r\n            roomProps: {\r\n                children: 'room',\r\n                label: 'name',\r\n                value: 'name'\r\n            },\r\n            classroom: [\r\n                {\r\n                    name: '第三教学楼',\r\n                    aka: 'H3',\r\n                    room: [\r\n                        {name:'301'},\r\n                        {name:'402'}\r\n                    ]\r\n                },\r\n                {\r\n                    name: '光华楼西辅楼',\r\n                    aka: 'HGX',\r\n                    room: [\r\n                        {name:'201'},\r\n                        {name:'502'}\r\n                    ]\r\n                },\r\n            ],\r\n            courses: [\r\n                {\r\n                    courseId: 1,\r\n                    courseName: '软件工程',\r\n                    courseNum: 'SOFT220011',\r\n                    classHours: '4',\r\n                    credits: '4',\r\n                    \r\n                    collegeId: '',\r\n                    collegeName: '计算机与技术学院',\r\n                    times: [\r\n                        [],\r\n                        [1,2,3],\r\n                        [3,5],\r\n                        []\r\n                    ],\r\n                    courseInfo: '123',\r\n                    status: 'normal',\r\n                },\r\n                {\r\n                    courseId: 2,\r\n                    courseName: '离散数学',\r\n                    courseNum: 'SOFT220022',\r\n                    classHours: '3',\r\n                    credits: '5',\r\n                    \r\n                    collegeId: '',\r\n                    collegeName: '计算机与技术学院',\r\n                    times: [\r\n                        [],\r\n                        [3,4],\r\n                        [3,8],\r\n                        []\r\n                    ],\r\n                    courseInfo: '123',\r\n                    status: 'normal',\r\n                }\r\n            ],\r\n            editCourse: {\r\n                index: '',\r\n                courseName: '',\r\n                classroom: '',\r\n                times: '',\r\n            }\r\n        }\r\n    },\r\n    methods: {\r\n        // eslint-disable-next-line\r\n        tableRowClassName(row, rowIndex) {//根据该行课程的状态动态显示该行表格颜色\r\n            if(row.row.status == 'changed')\r\n                return 'changing-row';\r\n            if(row.row.status == 'deleted')\r\n                return 'deleting-row';\r\n        },\r\n        handleDelete(index, data){\r\n            ElMessageBox.confirm(\r\n                '是否申请删除课程\"'+data.courseName+'\"',\r\n                '请确认',\r\n                {\r\n                confirmButtonText: '确认',\r\n                cancelButtonText: '取消',\r\n                type: 'warning',\r\n                }\r\n            )\r\n                .then(() => {\r\n                    data.status = 'deleted'\r\n                    ElMessage({\r\n                        type: 'success',\r\n                        message: '已发起申请',\r\n                    })\r\n                })\r\n                .catch(() => {\r\n                    ElMessage({\r\n                        type: 'info',\r\n                        message: '申请取消',\r\n                    })\r\n                })\r\n        },\r\n        handleEdit(index, data) {\r\n            //axios获取教室\r\n            this.editCourse.index = index\r\n            this.editCourse.courseName = data.courseName\r\n            this.editTableVisible = true;\r\n        },\r\n        submitEdit() {\r\n            \r\n            this.$refs['editCourse'].validate(valid => {\r\n                if(valid){\r\n                    this.courses[this.editCourse.index].courseName = this.editCourse.courseName\r\n                    this.courses[this.editCourse.index].status = 'changed'\r\n                    this.editTableVisible = false;\r\n                }\r\n            })\r\n\r\n        },\r\n        cancelEdit() {\r\n            this.editTableVisible = false;\r\n        },\r\n        test() {\r\n            this.courses[0].status = 'changed'\r\n        }\r\n    },\r\n}\r\n</script>\r\n\r\n<style>\r\n.el-table .deleting-row {\r\n  --el-table-tr-bg-color: var(--el-color-danger-light-7);\r\n}\r\n.deleting-row .el-table-fixed-column--left {\r\n    background: var(--el-color-danger-light-7) !important;\r\n}\r\n.el-table .success-row {\r\n  --el-table-tr-bg-color: var(--el-color-success-light-9);\r\n}\r\n.success-row .el-table-fixed-column--left {\r\n    background: var(--el-color-success-light-9) !important;\r\n}\r\n.el-table .changing-row {\r\n  --el-table-tr-bg-color: var(--el-color-warning-light-9);\r\n}\r\n.changing-row .el-table-fixed-column--left {\r\n    background: var(--el-color-warning-light-9) !important;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}