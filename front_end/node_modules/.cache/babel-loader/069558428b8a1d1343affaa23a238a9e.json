r\n                </el-dialog>\r\n            </template>\r\n        </el-table-column>\r\n        <el-table-column fixed=\"right\" label=\"Operations\" width=\"180\">\r\n        <template #default=\"scope\">\r\n            <el-button size=\"small\" @click=\"handleEdit(scope.$index, scope.row)\">申请修改</el-button>\r\n            <el-button size=\"small\" type=\"danger\" @click=\"handleDelete(scope.$index, scope.row)\">申请删除</el-button>\r\n        </template>\r\n        </el-table-column>\r\n    </el-table>  \r\n</template>\r\n\r\n<script>\r\nimport { ElMessage, ElMessageBox } from 'element-plus'\r\nexport default {\r\n    data() {\r\n        return {\r\n            dialogTableVisible:false,\r\n            courses: [\r\n                {\r\n                    courseId: 1,\r\n                    courseName: '软件工程',\r\n                    courseNum: 'SOFT220011',\r\n                    classHours: '4',\r\n                    credits: '4',\r\n                    \r\n                    collegeId: '',\r\n                    collegeName: '计算机与技术学院',\r\n                    weekday: '',\r\n                    courseInfo: '123',\r\n                    status: 'normal',\r\n                }\r\n            ]\r\n        }\r\n    },\r\n    methods: {\r\n        // eslint-disable-next-line\r\n        tableRowClassName(row, rowIndex) {//根据该行课程的状态动态显示该行表格颜色\r\n            if(row.row.status == 'changed')\r\n                \r\n                return 'warning-row';\r\n        },\r\n        handleDelete(index, data){\r\n            ElMessageBox.confirm(\r\n                '是否发起删除申请课程\"'+data.courseName+'\"',\r\n                '请确认',\r\n                {\r\n                confirmButtonText: 'OK',\r\n                cancelButtonText: 'Cancel',\r\n                type: 'warning',\r\n                }\r\n            )\r\n                .then(() => {\r\n                ElMessage({\r\n                    type: 'success',\r\n                    message: 'Delete completed',\r\n                })\r\n                })\r\n                .catch(() => {\r\n                ElMessage({\r\n                    type: 'info',\r\n                    message: 'Delete canceled',\r\n                })\r\n                })\r\n            data.status = 'changed'\r\n        },\r\n        test() {\r\n            this.courses[0].status = 'changed'\r\n        }\r\n    },\r\n}\r\n</script>\r\n\r\n<style>\r\n.el-table .warning-row {\r\n  --el-table-tr-bg-color: var(--el-color-warning-light-9);\r\n}\r\n.warning-row .el-table-fixed-column--left {\r\n    background: var(--el-color-warning-light-9) !important;\r\n}\r\n.el-table .success-row {\r\n  --el-table-tr-bg-color: var(--el-color-success-light-9);\r\n}\r\n.success-row .el-table-fixed-column--left {\r\n    background: var(--el-color-success-light-9) !important;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}