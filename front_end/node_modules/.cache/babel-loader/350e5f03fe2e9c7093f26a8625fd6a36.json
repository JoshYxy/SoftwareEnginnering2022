sD,KAAtD,CAAX,CAApB;AACJ;;AACA,WAAKjF,KAAL,GAAa+C,GAAG,CAAClE,IAAJ,CAASqG,KAAtB,CARS,CAST;AACA;AACA;AACA;AACA;AACA;AACH,KAhBK,EAgBHtB,KAhBG,CAgBGC,KAAI,IAAK;AACdrB,aAAO,CAACuB,GAAR,CAAYF,KAAZ;AACH,KAlBK,CAAN;AAmBA,SAAK9D,QAAL,GAAgB,EAAhB;;AACA,SAAI,IAAIoC,IAAI,CAAZ,EAAeA,IAAI,CAAnB,EAAsBA,CAAC,EAAvB,EAA2B;AAAC;AACxB,WAAKpC,QAAL,CAAcoF,IAAd,CAAmB;AAACvF,UAAE,EAAC,CAAJ;AAAOL,YAAI,EAAC,EAAZ;AAAgBS,aAAK,EAAC;AAAtB,OAAnB;AACA,WAAKD,QAAL,CAAcoC,CAAd,EAAiBvC,EAAjB,GAAsBuC,CAAtB;AACA,WAAKpC,QAAL,CAAcoC,CAAd,EAAiB5C,IAAjB,GAAwB,MAAK4C,CAA7B;AACA,WAAKpC,QAAL,CAAcoC,CAAd,EAAiBnC,KAAjB,GAAyB,EAAzB;;AACA,WAAI,IAAIqC,CAAR,IAAa,KAAKrC,KAAlB,EAAyB;AACrB,aAAKD,QAAL,CAAcoC,CAAd,EAAiBnC,KAAjB,CAAuBmF,IAAvB,CAA4B;AAACC,aAAG,EAAEC,QAAQ,CAAChD,CAAD,CAAR,GAAY,CAAlB;AAAoB9C,cAAI,EAAC,KAAKS,KAAL,CAAWqC,CAAX,EAAc9C,IAAvC;AAA6C2D,iBAAO,EAAC;AAArD,SAA5B;AACJ;AACJ;;AACAV,WAAO,CAACC,GAAR,CAAY,KAAK1C,QAAjB;AAEJ;;AAtWW,CAAf","names":["CourseTime","Check","Upload","validTimetable","validSelectRoom","validTeacher","validCollege","setCourseTime","axios","global_","ElMessage","components","data","buildingToAbbr","periods","roomProps","children","label","value","majorProps","multiple","classroom","name","aka","room","capacity","collegeData","id","majors","disabled","timeData","times","startTime","endTime","unavalTeaTimes","unavalRoomTimes","teacherData","collegeName","teachers","number","avalTeacher","courseData","selectTime","selectRoom","courseName","courseNum","college","period","credits","teacher","teacherNum","teacherName","courseInfo","time","position","building","roomNum","commonCourse","rules","required","message","trigger","pattern","validator","computed","roomCap","i","length","j","methods","test","console","log","updateCollege","updateMajors","updateTeacher","put","then","res","data1","indexOf","disable","p","splice","updateRoom","submit","$refs","validate","valid","post","type","catch","error","alert","dir","onBeforeUpload","file","isCSV","$message","param","para","FormData","append","headers","response","msg","created","get","data2","JSON","parse","stringify","replace","data3","push","num","parseInt"],"sourceRoot":"","sources":["D:\\fdu\\software_engineering\\Lab\\5-Lab\\front_end\\src\\components\\course\\CourseMaintenance.vue"],"sourcesContent":["<template>\r\n    <h2>新增课程</h2>\r\n    <el-upload\r\n      class=\"upload-demo\"\r\n      action=\"#\"\r\n      :http-request=\"Upload\"\r\n      accept=\".csv\"\r\n      :limit=\"1\"\r\n      :before-upload=\"onBeforeUpload\"\r\n    >\r\n      <el-button type=\"primary\">\r\n        <el-icon style=\"vertical-align: middle;\">\r\n          <upload />\r\n        </el-icon>\r\n        <span style=\"vertical-align: middle;\"> 上传文件 </span>\r\n      </el-button>\r\n      <template #tip>\r\n        <div class=\"el-upload__tip\">\r\n          单个csv文件\r\n        </div>\r\n      </template>\r\n    </el-upload>\r\n\r\n    <!-- <div><el-button @click=\"test\">\r\n        测试\r\n    </el-button></div> -->\r\n    <el-form class=\"new-course-admin\" :model=\"courseData\" ref=\"courseData\" :rules=\"rules\" hide-required-asterisk>\r\n        <el-form-item label=\"课程名称\" prop=\"courseName\">\r\n            <el-input v-model=\"courseData.courseName\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"课程编号\" prop=\"courseNum\">\r\n            <el-input v-model=\"courseData.courseNum\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"课程学分\" prop=\"credits\">\r\n            <el-input v-model=\"courseData.credits\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"选课容量\" prop=\"capacity\">\r\n            <el-input v-model=\"courseData.capacity\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"课程简介\" prop=\"courseInfo\">\r\n            <el-input v-model=\"courseData.courseInfo\" type=\"textarea\" />\r\n        </el-form-item>  \r\n        <el-form-item label=\"课程类型\" prop=\"commonCourse\">\r\n            <el-select v-model=\"courseData.commonCourse\" placeholder=\"类型\">\r\n                <el-option label=\"通选课程\" value=\"1\" />\r\n                <el-option label=\"专业选修\" value=\"0\" />\r\n            </el-select>\r\n        </el-form-item> \r\n        <el-form-item label=\"面向专业\" prop=\"majors\" v-if=\"courseData.commonCourse == '0'\">\r\n            <el-cascader :props=\"majorProps\" :options=\"collegeData\" v-model=\"courseData.majors\" placeholder=\"面向专业\" :show-all-levels='false' @change=\"updateMajors\" clearable/>\r\n        </el-form-item> \r\n        <el-form-item label=\"开课院系\" prop=\"college\">\r\n            <el-select v-model=\"courseData.college\" value-key=\"collegeName\" placeholder=\"学院\" @change=\"updateCollege\">\r\n                <el-option :key=\"college.collegeName\" :value=\"college\" :label=\"college.collegeName\" v-for=\"college in teacherData\" />\r\n            </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"任课教师\" prop=\"teacher\">\r\n            <el-select v-model=\"courseData.teacher\" value-key=\"number\" placeholder=\"教师\" no-data-text=\"未选择开课院系或学院无教师\" @change=\"updateTeacher\">\r\n                <el-option :key=\"teacher.number\" :value=\"teacher\" :label=\"teacher.name+teacher.number\" v-for=\"teacher in courseData.college.teachers\" />\r\n            </el-select>\r\n        </el-form-item> \r\n        <el-form-item label=\"上课教室\" prop=\"selectRoom\">\r\n            <el-cascader :props=\"roomProps\" :options=\"classroom\" v-model=\"courseData.selectRoom\" placeholder=\"可输入教室号搜索\" @change=\"updateRoom\" filterable clearable/>\r\n            <span>教室容量: {{roomCap}} </span>\r\n        </el-form-item>\r\n        <el-form-item label=\"上课时间\" prop=\"selectTime\">            \r\n            <div class=\"time-container\">\r\n                <div class=\"left-part\">\r\n                    <div class=\"line\" v-for=\"day in times\" :key=\"day.id\">{{day.startTime}}-{{day.endTime}}</div>\r\n                </div>\r\n                <div class=\"right-part\">\r\n                    <span class=\"class-week\" v-for=\"period in periods\" :key=\"period\">{{period}}</span> \r\n                        <div class=\"right-down\">\r\n                        <el-checkbox-group v-for=\"day in timeData\" :key=\"day.id\" v-model=\"courseData.selectTime[day.id]\">\r\n                            \r\n                            <el-checkbox-button \r\n                                v-for=\"time in day.times\" \r\n                                :key=\"time.num\" \r\n                                :label=\"time.num\" \r\n                                :disabled=\"time.disable\" \r\n                                style=\"display:block;\"\r\n                                >\r\n                                {{time.name}}\r\n                            </el-checkbox-button>\r\n                            <!-- <span class=\"day\"> {{day.name}} </span> -->\r\n                        </el-checkbox-group>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <el-button type=\"primary\" @click=\"submit\">提交</el-button>\r\n        </el-form-item>\r\n    </el-form>\r\n    <!-- <course-time>\r\n    </course-time> -->\r\n    <!-- <el-checkbox-group v-model=\"selectTime\" size=\"large\">\r\n      <el-checkbox-button v-for=\"time in times\" :key=\"time\" :label=\"time\">\r\n        {{ time }}\r\n      </el-checkbox-button>\r\n    </el-checkbox-group> -->\r\n</template>\r\n\r\n<script>\r\n/* eslint-disable */\r\nimport CourseTime from './CourseTime.vue'\r\nimport {Check,Upload} from \"@element-plus/icons\";\r\nimport {validTimetable, validSelectRoom, validTeacher, validCollege} from '../jsComponents/CheckRules'\r\nimport {setCourseTime} from '../jsComponents/CourseSet'\r\nimport axios from 'axios'\r\nimport global_ from '../jsComponents/global'\r\nimport {ElMessage} from 'element-plus'\r\nexport default {\r\n    components: {\r\n        CourseTime,\r\n        Check,\r\n        Upload\r\n    },\r\n    data() {\r\n        return {\r\n            buildingToAbbr: global_.buildingToAbbr,\r\n            periods:[\r\n                '周日','周一','周二','周三','周四','周五','周六',\r\n            ],\r\n            \r\n            roomProps: {\r\n                children: 'room',\r\n                label: 'name',\r\n                value: 'name'\r\n            },\r\n            majorProps: {\r\n                children: 'majors',\r\n                label: 'name',\r\n                value: 'name',\r\n                multiple: true\r\n            },\r\n            classroom: [\r\n                {\r\n                    name: '第三教学楼',\r\n                    aka: 'H3',\r\n                    room: [\r\n                        {name:'301', capacity:'120'},\r\n                        {name:'402', capacity:'120'}\r\n                    ]\r\n                },\r\n                {\r\n                    name: '光华楼西辅楼',\r\n                    aka: 'HGX',\r\n                    room: [\r\n                        {name:'201', capacity:'110'},\r\n                        {name:'502', capacity:'100'}\r\n                    ]\r\n                },\r\n            ],\r\n            collegeData: [\r\n                { \r\n                    id: 1,\r\n                    name: '计算机科学技术学院', \r\n                    majors: [\r\n                        {id: 1, name: '大数据'},\r\n                        {id: 2, name: '信息安全'}\r\n                    ]\r\n                },\r\n                { \r\n                    id: 1,\r\n                    name: '生命科学学院', \r\n                    disabled: true,\r\n                    majors: [\r\n                        // {id: 1, name: '生物'},\r\n                        // {id: 2, name: '123'}\r\n                    ]\r\n                },\r\n                { \r\n                    id: 1,\r\n                    name: '软件工程学院', \r\n                    majors: [\r\n                        {id: 1, name: '软件工程'},\r\n                    ]\r\n                },\r\n            ],\r\n            timeData:[\r\n            ],\r\n\r\n            times: [\r\n                {\r\n                    name: '第一节',\r\n                    startTime:'08:00',\r\n                    endTime:'08:40'\r\n                },\r\n                {\r\n                    name: '第二节',\r\n                    startTime:'09:00',\r\n                    endTime:'09:40'\r\n                },\r\n                {\r\n                    name: '第三节',\r\n                    startTime:'09:50',\r\n                    endTime:'10:30'\r\n                },\r\n                {\r\n                    name: '第四节',\r\n                    startTime:'10:40',\r\n                    endTime:'11:20'\r\n                },                \r\n            ],\r\n            unavalTeaTimes: [\r\n                [],\r\n                [],\r\n                [],\r\n                [],\r\n                [],\r\n                [],\r\n                []\r\n            ],\r\n            unavalRoomTimes: [\r\n                [],\r\n                [],\r\n                [],\r\n                [],\r\n                [],\r\n                [],\r\n                []\r\n            ],\r\n            teacherData: [\r\n                {\r\n                    collegeName:'计算机科学技术学院',\r\n                    teachers: [\r\n                        {name:'小王',number: 22200000}\r\n                    ]\r\n                },\r\n                {\r\n                    collegeName: '软件工程学院',\r\n                    teachers: [\r\n                        {name:'小一',number: 22111100},\r\n                        {name:'小零',number: 20000000},\r\n                    ]\r\n                }\r\n            ],\r\n            avalTeacher: [],\r\n            courseData: {\r\n                selectTime:[[],[],[],[],[],[],[]],\r\n                times:[[]],\r\n                selectRoom: '',\r\n                courseName: '',\r\n                courseNum: '',\r\n                college: {collegeName: '', teachers: []},\r\n                period: 0,\r\n                credits: 0,//学分\r\n                teacher: {name:'',number:''},\r\n                teacherNum: '',\r\n                teacherName: '',\r\n                courseInfo: '',\r\n                time: '',\r\n                position: '',\r\n                capacity: 0,\r\n                building: '',\r\n                roomNum: '',\r\n                commonCourse:'1',\r\n                majors: [\r\n                            [\"计算机科学技术学院\",\"大数据\"],\r\n                            [\"计算机科学技术学院\",\"信息安全\"],\r\n                            ['软件工程学院','软件工程']\r\n                        ]\r\n            },\r\n            rules: {\r\n                courseName: [{required: true, message: '请输入课程名称', trigger: 'blur'}],\r\n                courseNum: [{required: true, message: '请输入课程编号', trigger: 'blur'}],\r\n                credits: [{required: true, message: '请输入学分', trigger: 'blur'},\r\n                          {pattern:/^[1-9]\\d*$/, message: '请输入正整数', trigger: 'blur'}],\r\n                courseInfo: [{required: true, message: '请输入课程介绍', trigger: 'blur'}],\r\n                commonCourse: [{required: true, message: '请选择课程类型',trigger: ['blur','change']}],\r\n                majors: [{required: true, message: '请选择面向专业',trigger: ['blur','change']}],\r\n                capacity: [{required: true, message: '请输入选课容量', trigger: 'blur'},\r\n                           {pattern:/^[1-9]\\d*$/, message: '请输入正整数', trigger: 'blur'}],\r\n                college: [{validator: validCollege, trigger: ['blur','change']}],\r\n                teacher: [{validator: validTeacher, trigger: ['blur','change']}], \r\n                selectRoom: [{validator: validSelectRoom, trigger: ['blur','change']}],\r\n                selectTime: [{validator: validTimetable, trigger: ['blur','change']}],\r\n            }\r\n        }\r\n    },\r\n    computed: {\r\n        roomCap() {\r\n            for(let i = 0; i < this.classroom.length; 