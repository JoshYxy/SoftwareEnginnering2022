his.courses[index].type = 'changed';\n          this.editTableVisible[index] = false;\n        }\n      });\n    },\n\n    cancelEdit(index) {\n      this.editTableVisible[index] = false;\n    },\n\n    test() {\n      // this.courses[0].type = 'changed'\n      console.log(this.courses);\n    }\n\n  },\n\n  created() {\n    for (let course of this.courses) {\n      course.courseTime = '';\n      setCourseTime(course, course.times);\n    }\n\n    this.timeData = [];\n\n    for (let i = 0; i < 7; i++) {\n      //创建选课时间数组\n      this.timeData.push({\n        id: 0,\n        name: '',\n        times: []\n      });\n      this.timeData[i].id = i;\n      this.timeData[i].name = '周' + i;\n      this.timeData[i].times = [];\n\n      for (let j in this.times) {\n        this.timeData[i].times.push({\n          num: parseInt(j) + 1,\n          name: this.times[j].name,\n          disable: false\n        });\n      }\n    } // console.log(this.timeData)\n\n  }\n\n};","map":{"version":3,"mappings":"AA8GA;AACA,SAASA,SAAT,EAAoBC,YAApB,QAAwC,cAAxC;AACA,SAAQC,cAAR,EAAwBC,eAAxB,EAAyCC,YAAzC,QAA4D,4BAA5D;AACA,SAAQC,aAAR,QAA4B,2BAA5B;AACA,OAAOC,OAAP,MAAoB,wBAApB;AACA,eAAe;AACXC,MAAI,GAAG;AACH,WAAO;AACHC,WAAK,EAAE;AACHC,kBAAU,EAAE,CAAC;AAACC,kBAAQ,EAAE,IAAX;AAAiBC,iBAAO,EAAE,SAA1B;AAAqCC,iBAAO,EAAE,CAAC,MAAD,EAAQ,QAAR;AAA9C,SAAD,CADT;AAEHC,kBAAU,EAAE,CAAC;AAACH,kBAAQ,EAAE,IAAX;AAAiBC,iBAAO,EAAE,SAA1B;AAAqCC,iBAAO,EAAE,CAAC,MAAD,EAAQ,QAAR;AAA9C,SAAD,CAFT;AAGHE,iBAAS,EAAE,CAAC;AAACJ,kBAAQ,EAAE,IAAX;AAAiBC,iBAAO,EAAE,SAA1B;AAAqCC,iBAAO,EAAE,CAAC,MAAD,EAAQ,QAAR;AAA9C,SAAD,CAHR;AAIHG,eAAO,EAAE,CAAC;AAACL,kBAAQ,EAAE,IAAX;AAAiBC,iBAAO,EAAE,OAA1B;AAAmCC,iBAAO,EAAE,CAAC,MAAD,EAAQ,QAAR;AAA5C,SAAD,EACC;AAACI,iBAAO,EAAC,YAAT;AAAuBL,iBAAO,EAAE,QAAhC;AAA0CC,iBAAO,EAAE;AAAnD,SADD,CAJN;AAMHK,gBAAQ,EAAE,CAAC;AAACP,kBAAQ,EAAE,IAAX;AAAiBC,iBAAO,EAAE,SAA1B;AAAqCC,iBAAO,EAAE,CAAC,MAAD,EAAQ,QAAR;AAA9C,SAAD,EACC;AAACI,iBAAO,EAAC,YAAT;AAAuBL,iBAAO,EAAE,QAAhC;AAA0CC,iBAAO,EAAE;AAAnD,SADD,CANP;AAQHM,eAAO,EAAE,C