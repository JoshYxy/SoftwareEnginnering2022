\n\n  created() {\n    axios.get('http://localhost:8081/course/courseRequests').then(res => {\n      let requestData = res.data.data;\n      this.pendingCourses = [];\n\n      for (let i = 0; i < requestData.length; i++) {\n        this.pendingCourses.push(requestData[i].courseVO);\n        this.pendingCourses[i].type = requestData[i].type;\n        this.pendingCourses[i].requestId = requestData[i].requestId;\n        this.pendingCourses[i].examined = false;\n        this.pendingCourses[i].passed = false;\n        this.pendingCourses[i].showStatus = this.statusToShow[this.pendingCourses[i].type];\n      }\n    }); // this.pendingCourses[0].showStatus = this.statusToShow[this.pendingCourses[0].type]\n    // console.log(this.pendingCourses[0].type)\n  }\n\n};","map":{"version":3,"mappings":"AAsCA,OAAOA,KAAP,MAAkB,OAAlB;AACA,eAAe;AACXC,MAAI,GAAG;AACH,WAAO;AACHC,kBAAY,EAAE;AACVC,WAAG,EAAE,IADK;AAEVC,cAAM,EAAE,IAFE;AAGVC,cAAM,EAAE;AAHE,OADX;AAMHC,wBAAkB,EAAC,CAAC,KAAD,EAAO,KAAP,CANhB;AAOHC,oBAAc,EAAC,CACX;AACIC,kBAAU,EAAE,EADhB;AAEIC,gBAAQ,EAAE,CAFd;AAGIC,iBAAS,EAAE,CAHf;AAIIC,kBAAU,EAAE,MAJhB;AAKIC,iBAAS,EAAE,YALf;AAMIC,kBAAU,EAAE,UANhB;AAOIC,mBAAW,EAAE,IAPjB;AAQIC,kBAAU,EAAE,GARhB;AASIC,eAAO,EAAE,GATb;AAUIC,kBAAU,EAAC,6BAVf;AAWIC,gBAAQ,EAAE,KAXd;AAYIC,iBAAS,EAAE,EAZf;AAaIC,mBAAW,EAAE,UAbjB;AAcIC,aAAK,EAAE,CACH,EADG,EAEH,CAAC,CAAD,EAAG,CAAH,EAAK,CAAL,EAAO,CAAP,EAAS,CAAT,EAAW,CAAX,EAAa,EAAb,EAAgB,EAAhB,EAAmB,EAAnB,CAFG,EAGH,CAAC,CAAD,CAHG,EAIH,EAJG,EAKH,EALG,EAMH,EANG,EAOH,EAPG,CAdX;AAuBIC,gBAAQ,EAAE,IAvBd;AAwBIC,eAAO,EAAE,KAxBb;AAyBIC,kBAAU,EAAE,KAzBhB;AA0BIC,YAAI,EAAE,QA1BV;AA2BIC,gBAAQ,EAAE,KA3Bd;AA4BIC,cAAM,EAAE;AA5BZ,OADW,EA+BX;AACInB,kBAAU,EAAE,EADhB;AAEIC,gBAAQ,EAAE,CAFd;AAGIC,iBAAS,EAAE,CAHf;AAIIC,kBAAU,EAAE,IAJhB;AAKIC,iBAAS,EAAE,aALf;AAMIC,kBAAU,EAAE,UANhB;AAOIC,mBAAW,EAAE,IAPjB;AAQIC,kBAAU,EAAE,GARhB;AASIC,eAAO,EAAE,GATb;AAUIC,kBAAU,EAAC,6BAVf;AAWIC,gBAAQ,EAAE,KAXd;AAYIC,iBAAS,EAAE,EAZf;AAaIC,mBAAW,EAAE,UAbjB;AAcIC,aAAK,EAAE,CACH,EADG,EAEH,CAAC,CAAD,EAAG,CAAH,